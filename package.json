  },
  "homepage": "https://github.com/Spy-x-Family-FEC-Team/Atelier-FEC#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/src/components/__tests__",
      "\\.(css|less)$": "<rootDir>/client/src/stylesheets/",
      "^/(.*)$": "<rootDir>/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/client/src/stylesheets/"
    ],
    "testEnvironment": "jsdom"
  },
