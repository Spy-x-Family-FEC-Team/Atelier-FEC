{
  "name": "atelier-fec",
  "version": "1.0.0",
  "description": "A client-facing web portal designed to showcase products clearly and compellingly.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --watch",
    "test": "jest --coverage",
    "start": "npx nodemon --watch server server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spy-x-Family-FEC-Team/Atelier-FEC.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
